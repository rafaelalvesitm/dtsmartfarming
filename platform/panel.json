    },
    "id": 6,
    "targets": [
      {
        "refId": "A",
        "format": "table",
        "timeColumn": "time",
        "metricColumn": "none",
        "group": [],
        "where": [
          {
            "type": "macro",
            "name": "$__timeFilter",
            "params": []
          }
        ],
        "select": [
          [
            {
              "type": "column",
              "params": [
                "value"
              ]
            }
          ]
        ],
        "rawQuery": true,
        "rawSql": "SELECT\r\n  CAST(MAX(CASE attrName WHEN 'dateForecast' THEN attrValue END) as date) as \"time\",   \r\n  CAST(MAX(CASE attrName WHEN \"probability\" THEN attrValue END)as decimal(5,2)) as \"Precipitation Probability\",\r\n  CAST(MAX(CASE attrName WHEN \"temperatureMax\" THEN attrValue END) as decimal(5,2)) as \"Temperature Max\",\r\n  CAST(MAX(CASE attrName WHEN \"temperatureMin\" THEN attrValue END) as decimal(5,2)) as \"Temperature Min\",\r\n  CAST(MAX(CASE attrName WHEN \"humidity\" THEN attrValue END)as decimal(5,2)) as \"Humidity\",\r\n  CAST(MAX(CASE attrName WHEN \"pressure\" THEN attrValue END)as decimal(5,2)) as \"Pressure\", \r\n  CAST(MAX(CASE attrName WHEN \"windDeg\" THEN attrValue END) as decimal(5,2)) as \"Wind Degree\",   \r\n  CAST(MAX(CASE attrName WHEN \"windSpeed\" THEN attrValue END) as decimal(5,2)) as \"Wind Speed\" \r\n  FROM lab.WeatherForecast_WeatherForecast \r\n  GROUP BY recvTime \r\n  ORDER BY recvTime desc\r\n  limit 8;"
      }
    ],
    "options": {
      "showHeader": true,
      "sortBy": [
        {
